{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "outDir": "",
    "module": "commonjs",
    "target": "es5",
    "lib": ["es6", "dom"],
    "sourceMap": true,
    "jsx": "react",
    "moduleResolution": "node",
    "rootDir": "../",
    "baseUrl": "../",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "../src/**/*"
  ]
} 